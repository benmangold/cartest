{
  "name": "encoding-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm i; npm run build-client; npm run build-server; npm run build-db",
    "build-client": "docker build -f client/Dockerfile . -t bm/nginx-alpine",
    "build-db": "docker build -f mongo/Dockerfile -t bm/mongo-alpine . ",
    "build-server": "docker build -t bm/nodejs-ffmpeg . ",
    "ci": "npm run db; npm run server-ci; npm run client-ci",
    "cleanup": "./cleanup.sh",
    "client": "npm run lint-client-docker; npm run webpack-docker; npm run nginx",
    "client-ci": "npm run lint-client-docker; npm run webpack-docker; npm run nginx-ci",
    "client-dev": "npm run nginx; npm run webpack-watch-docker",
    "client-logs": "docker logs -f client",
    "db": "./mongo/start_mongo_container.sh",
    "db-logs": "docker logs -f mongo",
    "dev": "npm run db; npm run server-dev; npm run client-dev",
    "lint-client": "eslint client/src --fix",
    "lint-client-docker": "./client/eslint/eslint.sh",
    "nginx": "./client/start_nginx_container.sh",
    "nginx-ci": "./client/start_nginx_container_ci.sh",
    "server": "./server/start_nodejs_container.sh",
    "server-ci": "./server/start_nodejs_container_ci.sh",
    "server-dev": "./server/start_nodejs_container_dev.sh",
    "server-logs": "docker logs -f nodejs",
    "server-watch": "npx nodemon server/index.js",
    "startup": "npm run db; npm run server; npm run client",
    "test": "jest --detectOpenHandles",
    "webpack": "webpack --mode production",
    "webpack-docker": "./client/webpack/webpack.sh",
    "webpack-watch": "webpack --watch",
    "webpack-watch-docker": "./client/webpack/watch.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.517.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.3.1",
    "multer": "^1.4.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "webpack": "4.28.4",
    "webpack-cli": "^3.3.9"
  }
}
